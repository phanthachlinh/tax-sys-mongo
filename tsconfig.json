{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "esNext",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "types" : ["express","jest","graphql"],
    "noImplicitThis":true,
    "strictNullChecks":true,
    "strictPropertyInitialization":true,
    "strictBindCallApply":true,
    "strictFunctionTypes":true,
    "moduleResolution": "node"
  }
}
